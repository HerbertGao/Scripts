name: Blued闪照
desc: Blued闪照转通知

http:
  mitm:
    - "*burn-chatfiles.bldimg.com"
  script:
    - match: ^https:\/\/.*burn-chatfiles\.bldimg\.com\/
      name: blued.js
      type: response
      require-body: true

script-providers:
  blued.js:
    url: https://raw.githubusercontent.com/HerbertGao/Scripts/dev/Scripts/Blued/blued.js
    interval: 86400
